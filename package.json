{
  "name": "vape-timer",
  "version": "0.0.1",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "next lint",
    "prettier": "prettier ./src --write",
    "test": "jest",
    "test-watch": "jest --watch",
