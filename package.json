    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/eslint-plugin": "^11.7.0",
    "@next/eslint-plugin-next": "^12.0.7",
    "@testing-library/jest-dom": "^5.16.1",
